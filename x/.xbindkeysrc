# -- .xbindkeysrc -------------------------------------------------- {{{

"xbacklight -inc 20"
  XF86MonBrightnessUp

"xbacklight -dec 20"
  XF86MonBrightnessDown

"amixer set Master -5%"
  XF86AudioLowerVolume

"amixer set Master +5%"
  XF86AudioRaiseVolume

"amixer set Master toggle"
  XF86AudioMute

# }}}

# vim:foldmethod=marker:filetype=conf

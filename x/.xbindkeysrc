# -- .xbindkeysrc -------------------------------------------------- {{{

"light -A 10"
  XF86MonBrightnessUp

"light -U 10"
  XF86MonBrightnessDown

"amixer --quiet sset Master 5-"
  XF86AudioLowerVolume

"amixer --quiet sset Master 5+"
  XF86AudioRaiseVolume

"amixer --quiet sset Master toggle"
  XF86AudioMute

"scrot ~/Pictures/Screenshots/%Y%m%d-%H%M%S-screenshot.png"
  Print

# }}}

# vim:foldmethod=marker:filetype=conf
